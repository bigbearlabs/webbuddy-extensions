// ## chrome extension manifest.

{
  "name": "CHANGE THIS : Extension boilerplate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.io",
  "homepage_url": "http://bigbearlabs.com/research",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": true
  },
  // "options_page": "options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "browser_action/browser_action.html"
  },
  "chrome_url_overrides": {
    "history": "history/index.html"
  },
  "permissions": [
    "history",
    "tabs",
    "file:///*",
    "http://localhost/*"
  ],
  "omnibox": {
    "keyword": "extensionizr"
  }
  // ,
  // "content_security_policy": "script-src 'self' http://localhost 'unsafe-eval'; object-src 'self'"

